{
    "manifestVersion": 1,
    "id": "sfpowerscripts-release-extension",
    "publisher": "dxatscale",
    "name": "sfpowerscripts-release-extension",
    "version": "1.0.0",
    "description": "Additonal capability for Release Orchestration when used with sfpowerscripts",
    "tags": [
        "Extension",
        "Salesforce",
        "SFDX",
        "Publish",
        "Package",
        "Install",
        "Continuous Integration",
        "Continuous Delivery",
        "Build",
        "Release",
        "Azure Pipelines",
        "Azure DevOps Extensions",
        "Visual Studio Extensions"
    ],
    "icons": {
        "default": "tools-icon.png"
    },
    "public": false,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "scopes": [
        "vso.build_execute",
        "vso.release_manage",
        "vso.serviceendpoint_manage"
    ],
    "categories": [
        "Azure Pipelines"
    ],
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE.txt"
        }
    },
    "links": {
        "home": {
            "uri": "https://www.sfpowerscripts.com/"
        },
        "getstarted": {
            "uri": "https://www.sfpowerscripts.com/gettingstarted/"
        },
        "repository": {
            "uri": "https://github.com/Accenture/sfpowerscripts"
        },
        "issues": {
            "uri": "https://github.com/Accenture/sfpowerscripts/issues"
        },
        "support": {
            "uri": "https://sfpowerscripts.com/support/"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/Accenture/sfpowerscripts"
    },
    "badges": [
        {
            "href": "https://dev.azure.com/dxatscale/sfpowerscripts/_build?definitionId=40",
            "uri": "https://dev.azure.com/dxatscale/sfpowerscripts/_apis/build/status/sfpowerscripts?branchName=master",
            "description": "sfpowerscripts pipeline"
        }
    ],
    "branding": {
        "color": "rgb(255, 255, 255)",
        "theme": "light"
    },
    "files": [

        {
            "path": "BuildTasks/GenerateChangelogTask"
        }
    ],
    "contributions": [
        {
            "id": "sfpowerscripts-generatechangelog-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "BuildTasks/GenerateChangelogTask"
            }
        }
    ]
}
