{
    "compileOnSave": true,
    "compilerOptions": {
      "jsx": "react",
      "module": "commonjs",
      "target": "es6",
      "lib": [ "es6" ],
      "sourceMap": true,
      "strict": false,
      "strictNullChecks": false,
      "removeComments": true,
      "resolveJsonModule": true,
      "esModuleInterop": true  
    },
    "exclude": [
      "node_modules",
      "lib"
    ]
  }
  
  